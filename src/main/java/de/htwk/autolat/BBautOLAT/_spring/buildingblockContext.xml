<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="autolat" class="de.htwk.autolat.BBautOLAT.BBautOLATCourseNodeConfiguration" scope="prototype">
		<property name="enabled" value="${course.node.checklist.enabled}" />
	<property name="order"   value="220" />
	</bean>

	<bean id="autolatDBMappings" class="org.olat.core.commons.persistence.AdditionalDBMappings">
		<property name="xmlFiles">
			<list>								
                <value>de/htwk/autolat/LivingTaskInstance/LivingTaskInstanceImpl.hbm.xml</value>
                <value>de/htwk/autolat/tools/Scoring/ScoreObjectImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskSolution/TaskSolutionImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskModule/TaskModuleImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskConfiguration/TaskConfigurationImpl.hbm.xml</value>
                <value>de/htwk/autolat/Student/StudentImpl.hbm.xml</value>
                <value>de/htwk/autolat/Configuration/ConfigurationImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskResult/TaskResultImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskType/TaskTypeImpl.hbm.xml</value>
                <value>de/htwk/autolat/TaskInstance/TaskInstanceImpl.hbm.xml</value>
                <value>de/htwk/autolat/ServerConnection/ServerConnectionImpl.hbm.xml</value>
			</list>
		</property>
	</bean>	
</beans>
