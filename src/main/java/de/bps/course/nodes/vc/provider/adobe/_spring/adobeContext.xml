<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" 
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
  http://www.springframework.org/schema/context 
  http://www.springframework.org/schema/context/spring-context-3.0.xsd">
  
  <bean id="adobeDefaultConfig" class="de.bps.course.nodes.vc.provider.adobe.AdobeConnectConfiguration">
  	<!-- general -->
  	<property name="createMeetingImmediately" value="false" />
  	<!-- Adobe Connect specific -->
  	<property name="guestAccessAllowed" value="true" />
	<property name="guestStartMeetingAllowed" value="true" />
	<property name="useMeetingDates" value="false" />
  </bean>
  
</beans>