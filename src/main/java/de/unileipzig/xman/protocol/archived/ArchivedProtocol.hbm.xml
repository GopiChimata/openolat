<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.unileipzig.xman.protocol.archive.ArchivedProtocol" table="o_xman_archived_protocol">  
		<id name="key" column="id" type="long" unsaved-value="null">
			<generator class="hilo" />
		</id>
	
		<property name="identifier" column="identifier" type="string" />
		<property name="name" column="name" type="string" />
		<property name="date" column="date" type="date" />
		<property name="location" column="identifier" type="string" />
		<property name="comment" column="identifier" type="string" />
		<property name="result" column="identifier" type="string" />
		<property name="studyPath" column="identifier" type="string" />
		
		<timestamp name="lastModified" column="lastmodified" />   
		<timestamp name="creationDate" column="creationdate" />
	</class>
</hibernate-mapping>
