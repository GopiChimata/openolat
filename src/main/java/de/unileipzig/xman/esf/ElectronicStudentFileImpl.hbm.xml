<?xml version="1.0"?>
<!--
/**
 * 
 * Mapping of ElectronicStundentFileImpl class to database tables
 */
-->
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
  <class name="de.unileipzig.xman.esf.ElectronicStudentFileImpl" table="o_xman_esf">  
  
		<cache usage="read-write" />

    <id name="key" column="esf_id" type="long" unsaved-value="null">
      <generator class="hilo"/>
    </id>
    
   	<timestamp name="lastModified" column="lastmodified" />   
  	<property  name="creationDate" column="creationdate" type="timestamp" />   
        
    <list name="protocolList" cascade="none">
            <key column="parent_id"/>
            <index column="idx"/>
            <one-to-many class="de.unileipzig.xman.protocol.ProtocolImpl"/>
    </list>
    
     <many-to-one name="identity" class="org.olat.basesecurity.IdentityImpl" 
		column="identity_id" outer-join="true" cascade="none" not-null="false"/> 
    
     <many-to-one name="comments" class="de.unileipzig.xman.comment.CommentImpl" 
		column="comment_id" outer-join="true" cascade="all" not-null="false"/> 
		
  </class>  
</hibernate-mapping>