<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd">
  
	
	<!-- OLAT sites -->
	<bean id="olatsites" class="org.olat.core.gui.control.navigation.SiteDefinitions" />
	
	<!-- classes implementing the SiteDefinition interface -->

	<!-- Disable site by adding '<property name="enabled" value="false"/>' -->
	<bean id="olatsites_home" class="org.olat.home.HomeSiteDef" scope="prototype" >
	   <property name="order" value="${site.home.order}" />
	   <property name="enabled" value="${site.minimalhome.enabled}"/>
	</bean>
	<bean id="olatsites_groups" class="org.olat.group.site.GroupsSiteDef" scope="prototype" >
	   <property name="order" value="4" />
	   <property name="enabled" value="${site.groups.enable}"/>
	</bean>
	<bean id="olatsites_catalog" class="org.olat.catalog.site.CatalogSiteDef" scope="prototype" >
	   <property name="order" value="5" />
	   <property name="enabled" value="${site.catalog.enable}"/>
	</bean>
	<bean id="olatsites_repository" class="org.olat.repository.site.RepositorySiteDef" scope="prototype" >
	   <property name="order" value="6" />
	   <property name="enabled" value="${site.repository.enable}"/>
	</bean>
	<bean id="olatsites_qpool" class="org.olat.modules.qpool.site.QuestionPoolSiteDef" scope="prototype" >
	   <property name="order" value="7" />
	   <property name="enabled" value="${site.questionpool.enable}"/>
	</bean>
	<bean id="olatsites_useradmin" class="org.olat.admin.site.UserAdminSiteDef" scope="prototype" >
	   <property name="order" value="9" />
	</bean>
	<bean id="olatsites_admin" class="org.olat.admin.site.AdminSiteDef" scope="prototype">
	   <property name="order" value="10" />
	</bean>
	<bean id="olatsites_guidemo" class="org.olat.gui.demo.site.GUIDemoSiteDef" scope="prototype" >
	   <property name="order" value="11" />
	   <property name="enabled" value="${guidemo.enabled}"/>
	</bean>

</beans>