<?xml version="1.0" encoding="utf-8"?>
<list>
	<!-- document structure tags -->
	<tag name="html"/>
	<tag name="head"/>
	<tag name="meta"/>
	<tag name="body">
		<attribute name="onload"/>
	</tag>
	
	<!-- formating tags -->
	<tag name="h1"/>
	<tag name="h2"/>
	<tag name="h3"/>
	<tag name="h4"/>
	<tag name="h5"/>
	<tag name="h6"/>
	<tag name="p"/>
	<tag name="table">
		<attribute name="cellpadding"/>
